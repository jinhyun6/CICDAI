{
  "name": "cicdai-frontend",
  "public": true,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vue",
  "github": {
    "silent": true
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}